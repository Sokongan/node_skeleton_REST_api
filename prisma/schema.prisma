generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model User {
  id String @id @default(dbgenerated("gen_random_uuid()")) @db.Uuid
  firstName String  @db.VarChar(256) @map("first_name")
  middleName String?  @db.VarChar(256) @map("middle_name") 
  lastName  String  @db.VarChar(256) @map("last_name")
  username  String @unique 
  password  String
  isActive  Boolean @default(true)

  @@map("tbl_user")
}
